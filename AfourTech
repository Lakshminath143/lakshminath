from selenium import webdriver
from selenium.webdriver.common.by import By
driver = webdriver.Chrome("c://drivers//chromedriver.exe")
driver.get("https://www.flightradar24.com/data/airports/pnq")
# Navigating to arrivals section
# it is dynamic webtable
driver.find_elemets(By.XPATH,"//*[text(),'Arrivals']/").click()
# clicking alert continue box
driver.find_element(By.XPATH,"//*[text(),'CONTINUE']").click()
listOfStatus=driver.find_elements(By.XPATH,"//*[@class='table table-condensed table-hover data-table m-n-t-15']/tbody/tr/td[7]/span]/thead/tr/td").text()
for status in listOfStatus:
    print(status)

# # python program
def stars():
    res = []
    res1 = []
    s = "*"
    d = range(4)
    for i in range(5):
        res.append(i*s)
    print(res)
    for e in d[::-1]:
        res1.append(e*s)
    print(res1)
    f = res+res1
    for k in f:
        print(k)
stars()
    
